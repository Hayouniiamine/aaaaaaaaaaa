{
	"name": "tiktak-rag-worker",
	"main": "src/index.ts",
	"compatibility_date": "2026-01-31",
	// Workers AI
	"ai": {
		"binding": "AI"
	},
	// Vectorize
	"vectorize": [
		{
			"binding": "TIKTAK_KB",
			"index_name": "tiktak-kb"
		},
		{
			"binding": "TIKTAK_PLAYBOOKS",
			"index_name": "tiktak-playbooks-v2"
		}
	],
	// R2 bucket
	"r2_buckets": [
		{
			"binding": "TIKTAK_DOCS",
			"bucket_name": "tiktak-docs"
		}
	],
	"vars": {
		"INTERNAL_SHARED_SECRET": "dev-secret"
	}
}